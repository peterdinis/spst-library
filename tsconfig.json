{
	"compilerOptions": {
		"esModuleInterop": true,
		"skipLibCheck": true,
		"target": "es2022",
		"allowJs": true,
		"resolveJsonModule": true,
		"isolatedModules": true,
		"strict": true,
		"noUncheckedIndexedAccess": true,
		"checkJs": true,
		"lib": ["dom", "dom.iterable", "ES2022"],
		"module": "CommonJS",
		"moduleResolution": "Node",
		"jsx": "preserve",
		"types": ["bun-types"],
		"plugins": [
			{
				"name": "next"
			}
		],
		"incremental": true,
		"baseUrl": ".",
		"paths": {
			"~/*": ["./src/*"]
		},
		"noEmit": true
	},
	"include": [
		".eslintrc.cjs",
		"next-env.d.ts",
		"**/*.ts",
		"**/*.tsx",
		"**/*.cjs",
		"**/*.js",
		".next/types/**/*.ts"
, "next.config.mjs"	],
	"exclude": ["node_modules"]
}
